<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>DaJet.Data.Provider</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>DaJet.DataProvider</AssemblyName>
    <Version>1.1.1</Version>
    <Authors>Zhichkin</Authors>
    <Description>ADO .NET data provider for 1C:Enterprise 8.
Microsoft SQL Server and PostgreSQL databases are supported.</Description>
    <Copyright>Zhichkin Â© 2022</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/zhichkin/dajet/tree/main/src/dajet-data-provider</RepositoryUrl>
    <PackageProjectUrl>https://zhichkin.github.io/data-provider/main.html</PackageProjectUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\dajet-scripting\dajet-scripting.csproj" />
  </ItemGroup>

</Project>
